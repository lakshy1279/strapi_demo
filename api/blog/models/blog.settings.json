{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "blogtitle": {
      "type": "string",
      "unique": true
    },
    "content": {
      "type": "richtext",
      "unique": true
    },
    "image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "categories": {
      "via": "blogs",
      "collection": "categories"
    }
  }
}
